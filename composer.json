{
  "name": "jord-jd/dotenv-loader",
  "type": "library",
  "description": "Dotenv loader is a simple PHP library that will automatically load in a `.env` file from the root of your project into environment variables.",
  "keywords": [
    "dotenv",
    ".env",
    "environment",
    "library",
    "PHP",
    "PHP library",
    ".env PHP library"
  ],
  "homepage": "https://github.com/Jord-JD/dotenv-loader",
  "license": "LGPL-3.0-only",
  "require": {
    "php": ">=5.5.9",
    "vlucas/phpdotenv": "2.*"
  },
  "autoload": {
    "files": [
      "src/DotEnvLoader.php"
    ]
  },
  "replace": {
    "divineomega/dotenv-loader": "self.version"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "4.0-dev"
    }
  },
  "require-dev": {}
}
